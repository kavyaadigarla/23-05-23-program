#include<stdio.h>
main()
{
    int i,n,r,s,f,temp;
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        r=n%10;
        f=1,i=1;
        while(i<=r)
        {
            f=f*i;
            i++;
        }
        s=s+f;
        n=n/10;
    }
    if(s==temp)
    printf("Strong number");
    else
    printf("Not a strong number");
}
